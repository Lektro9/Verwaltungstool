{
   "type": "sqlite",
   "database": "personenverwaltung.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "./model/*.entity.ts",
      "build/**/*.entity.js"
   ],
   "migrations": [
      "./migration/**/*.ts"
   ],
   "subscribers": [
      "./subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "./model",
      "migrationsDir": "./migration",
      "subscribersDir": "./subscriber"
   }
}