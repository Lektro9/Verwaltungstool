{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "./model/**/*.js",
      "./model/**/*.ts",
      "./build/model/**/*.js"
      ],
   "migrations": [
      "./migration/**/*.ts"
   ],
   "subscribers": [
      "./subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "./model",
      "migrationsDir": "./migration",
      "subscribersDir": "./subscriber"
   }
}